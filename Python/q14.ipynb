   "source": [
    "import math\n",
    "H = int(input(\"Enter hour\"))\n",
    "M = int(input(\"Enter Minute\"))\n",
    "hr_angle = (H + M/60) *30\n",
    "min_angle = M*6\n",
    "angle = (abs(hr_angle-min_angle))\n",
    "value =  math.floor(min(angle ,360-angle))\n",
    "print(value)\n"
   ]
