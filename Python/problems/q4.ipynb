   "source": [
    "num = int(input(\"Enter a number : \"))\n",
    "sum = 0\n",
    "while num>0 :\n",
    "    rem = num % 10\n",
    "    sum = rem + sum\n",
    "    num =  num // 10\n",
    "print(sum)"
   ]
